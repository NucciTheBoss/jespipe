{
    "algorithms": {
        "LSTM": {
            "sequence_length": 12,
            "batch_size": 256,
            "epochs": 50,
            "validation_split": 0.1,
            "verbose": 1,
            "learning_rate": 0.001
        }
    },

    "plugins": {
        "LSTM": "plugins/models/RNN/LSTM.py",
        "CW_L2": "plugins/attack/carlinil2.py"
    },

    "datamanips": {
        "xgboost": {
            "n_features": 11
        },

        "randomforest": {
            "placeholder": 1
        },

        "pca": {
            "n_features": 11
        },

        "candlestick": {
            "time_interval": 20
        },

        "vanilla": {
            "placeholder": 1
        }
    },

    "attacks": {
        "CW_L2": {
            "max_change": 0.2,
            "min_change": 0.05,
            "change_step": 0.025,
