<?xml version="1.0" encoding="UTF-8"?>
<simu>
    <train>
        <dataset file="/path/to/some/file.csv">
            <model plugin="/path/to/my/model/build_code.py">
                <name>my_model_name</name>

                <algorithm>LSTM</algorithm>

                <parameters>
                    <squence_length>12</squence_length>
                    <batch_size>256</batch_size>
                    <epochs>50</epochs>
                    <validation_split>0.1</validation_split>
                    <verbose>1</verbose>
                    <learning_rate>0.001</learning_rate>
                </parameters>
                
                <xgboost tag="xgb1">
                    <n_features>11</n_features>
                </xgboost>

                <randomforest tag="rf1"></randomforest>
                
                <pca tag="pca1">
                    <n_features>11</n_features>
                </pca>

                <candlestick tag="cand1">
                    <time_interval>20</time_interval>
                </candlestick>
            </model>
        </dataset>

    </train>
    <attack>
        <dataset file="/path/to/some/file.csv">
            <cw_inf tag="cw_inf1" plugin="/path/to/cw_inf.py">
                <confidence>0.0</confidence>
                <targeted>0</targeted>
                <learning_rate>0.01</learning_rate>
                <max_iter>10</max_iter>
                <max_halving>5</max_halving>
                <max_doubling>5</max_doubling>
                <max_eps>0.3</max_eps>
                <eps_step>0.1</eps_step>
                <batch_size>128</batch_size>
                <verbose>1</verbose>
            </cw_inf>

            <bim tag="bim1" plugin="/path/to/bim.py">
                <max_eps>0.3</max_eps>
                <eps_step>0.1</eps_step>
                <targeted>0</targeted>
                <batch_size>32</batch_size>
                <verbose>1</verbose>
            </bim>

            <fgsm tag="fgsm1" plugin="/path/to/fgsm.py">
                <norm>inf</norm>
                <max_eps>0.3</max_eps>
                <eps_step>0.1</eps_step>
                <targeted>0</targeted>
                <num_random_int>0</num_random_int>
                <batch_size>32</batch_size>
                <minimal>0</minimal>
            </fgsm>
        </dataset>
    </attack>
 
    <clean>
        <clear_tmp></clear_tmp>
        <plot plugin="/path/to/plotting/function.py">
            <tag>xbg1</tag>
            <tag>rf1</tag>
            <tag>pca1</tag>
            <tag>cand1</tag>
        </plot>
        <compress>
            <format>gzip</format>
            <name>run1</name>
            <path>~/Documents</path>
        </compress>
    </clean>
</simu>
