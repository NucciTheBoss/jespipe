<?xml version="1.0" encoding="UTF-8"?>
<simu>
    <train>
        <dataset file="test/datasets/google-stock/google-clean.csv">
            <model plugin="plugins/LSTM/LSTM.py">
                <name>google-stock-LSTM</name>

                <algorithm>LSTM</algorithm>

                <parameters>
                    <sequence_length>12</sequence_length>
                    <batch_size>256</batch_size>
                    <epochs>50</epochs>
                    <validation_split>0.1</validation_split>
                    <verbose>1</verbose>
                    <learning_rate>0.001</learning_rate>
                </parameters>
                
                <xgboost tag="xgb1">
                    <n_features>3</n_features>
                </xgboost>

                <randomforest tag="rf1"></randomforest>
                
                <pca tag="pca1">
                    <n_features>3</n_features>
                </pca>

                <candlestick tag="cand1">
                    <time_interval>20</time_interval>
                </candlestick>
            </model>
        </dataset>

        <dataset file="test/datasets/ozone-level-detection/ozone-level-clean.csv">
            <model plugin="plugins/LSTM/LSTM.py">
                <name>ozone-avg-wind-speed</name>

                <algorithm>LSTM</algorithm>

                <parameters>
                    <sequence_length>12</sequence_length>
                    <batch_size>256</batch_size>
                    <epochs>50</epochs>
                    <validation_split>0.1</validation_split>
                    <verbose>1</verbose>
                    <learning_rate>0.001</learning_rate>
                </parameters>

                <xgboost tag="xgb1-ozone">
                    <n_features>11</n_features>
                </xgboost>
                
                <randomforest tag="rf1-ozone"></randomforest>

                <pca tag="pca1-ozone">
                    <n_features>11</n_features>
                </pca>

                <candlestick tag="cand1-ozone">
                    <time_interval>20</time_interval>
                </candlestick>
            </model>
        </dataset>

    </train>
    <attack>
        <dataset file="test/datasets/google-stock/google-clean.csv">
            <cw_inf tag="cw_inf1">
                <confidence>0.0</confidence>
                <targeted>0</targeted>
                <learning_rate>0.01</learning_rate>
                <max_iter>10</max_iter>
                <max_halving>5</max_halving>
                <max_doubling>5</max_doubling>
                <max_eps>0.3</max_eps>
                <eps_step>0.1</eps_step>
                <batch_size>128</batch_size>
                <verbose>1</verbose>
            </cw_inf>

            <bim tag="bim1">
                <max_eps>0.3</max_eps>
                <eps_step>0.1</eps_step>
                <targeted>0</targeted>
                <batch_size>32</batch_size>
                <verbose>1</verbose>
            </bim>

            <fgsm tag="fgsm1">
                <norm>inf</norm>
                <max_eps>0.3</max_eps>
                <eps_step>0.1</eps_step>
                <targeted>0</targeted>
                <num_random_int>0</num_random_int>
                <batch_size>32</batch_size>
                <minimal>0</minimal>
            </fgsm>
        </dataset>

        <dataset file="test/datasets/ozone-level-detection/ozone-level-clean.csv">
            <cw_inf tag="cw_inf1-ozone">
                <confidence>0.0</confidence>
                <targeted>0</targeted>
                <learning_rate>0.01</learning_rate>
                <max_iter>10</max_iter>
                <max_halving>5</max_halving>
                <max_doubling>5</max_doubling>
                <max_eps>0.3</max_eps>
                <eps_step>0.1</eps_step>
                <batch_size>128</batch_size>
                <verbose>1</verbose>
            </cw_inf>

            <bim tag="bim1-ozone">
                <max_eps>0.3</max_eps>
                <eps_step>0.1</eps_step>
                <targeted>0</targeted>
                <batch_size>32</batch_size>
                <verbose>1</verbose>
            </bim>

            <fgsm tag="fgsm1-ozone">
                <norm>inf</norm>
                <max_eps>0.3</max_eps>
                <eps_step>0.1</eps_step>
                <targeted>0</targeted>
                <num_random_int>0</num_random_int>
                <batch_size>32</batch_size>
                <minimal>0</minimal>
            </fgsm>
        </dataset>
    </attack>
 
    <clean>
    </clean>
</simu>