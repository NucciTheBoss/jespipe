<?xml version="1.0" encoding="UTF-8"?>
<simu>
    <train>
        <dataset file="test/datasets/google-stock/google-clean.csv">
            <model plugin="plugins/models/RNN/LSTM.py">
                <name>google-stock-LSTM</name>

                <algorithm>LSTM</algorithm>

                <parameters>
                    <sequence_length>12</sequence_length>
                    <batch_size>256</batch_size>
                    <epochs>50</epochs>
                    <validation_split>0.1</validation_split>
                    <verbose>1</verbose>
                    <learning_rate>0.001</learning_rate>
                </parameters>
                
                <xgboost tag="xgb1">
                    <n_features>3</n_features>
                </xgboost>

                <randomforest tag="rf1"></randomforest>
                
                <pca tag="pca1">
                    <n_features>3</n_features>
                </pca>

                <candlestick tag="cand1">
                    <time_interval>20</time_interval>
                </candlestick>
            </model>
        </dataset>

        <dataset file="test/datasets/ozone-level-detection/ozone-level-clean.csv">
            <model plugin="plugins/models/RNN/LSTM.py">
                <name>ozone-avg-wind-speed</name>

                <algorithm>LSTM</algorithm>

                <parameters>
                    <sequence_length>12</sequence_length>
                    <batch_size>256</batch_size>
                    <epochs>50</epochs>
                    <validation_split>0.1</validation_split>
                    <verbose>1</verbose>
                    <learning_rate>0.001</learning_rate>
                </parameters>

                <xgboost tag="xgb1-ozone">
                    <n_features>11</n_features>
                </xgboost>
                
                <randomforest tag="rf1-ozone"></randomforest>

                <pca tag="pca1-ozone">
                    <n_features>11</n_features>
                </pca>

                <candlestick tag="cand1-ozone">
                    <time_interval>20</time_interval>
                </candlestick>
            </model>
        </dataset>
    </train>
</simu>
