<?xml version="1.0" encoding="UTF-8"?>
<simu>
    <train>
        <dataset file="test/datasets/google-stock/google-clean.csv">
            <model plugin="plugins/models/RNN/LSTM.py">
                <name value="google-stock-LSTM" />

                <algorithm value="LSTM" />

                <parameters>
                    <sequence_length type="int" value="12" />
                    <batch_size type="int" value="256" />
                    <epochs type="int" value="50" />
                    <validation_split type="float" value="0.1" />
                    <verbose type="bool" value="True" />
                    <learning_rate type="float" value="0.001" />
                </parameters>
                
                <xgboost plugin="plugins/manip/many_to_one_xgb.py" tag="xgb1">
                    <n_features type="int" value="3" />
                </xgboost>

                <randomforest plugin="plugins/manip/many_to_one_randomforest.py" tag="rf1"></randomforest>
                
                <pca plugin="plugins/manip/many_to_one_pca.py" tag="pca1">
                    <n_features type="int" value="3" />
                </pca>

                <candlestick plugin="plugins/manip/many_to_one_candlestick.py" tag="cand1">
                    <time_interval type="int" value="20" />
                </candlestick>
            </model>
        </dataset>

        <dataset file="test/datasets/ozone-level-detection/ozone-level-clean.csv">
            <model plugin="plugins/models/RNN/LSTM.py">
                <name value="ozone-avg-wind-speed" />

                <algorithm value="LSTM" />

                <parameters>
                    <sequence_length type="int" value="12" />
                    <batch_size type="int" value="256" />
                    <epochs type="int" value="50" />
                    <validation_split type="float" value="0.1" />
                    <verbose type="bool" value="True" />
                    <learning_rate type="float" value="0.001" />
                </parameters>

                <xgboost plugin="plugins/manip/many_to_one_xgb.py" tag="xgb1-ozone">
                    <n_features type="int" value="11" />
                </xgboost>
                
                <randomforest plugin="plugins/manip/many_to_one_randomforest.py" tag="rf1-ozone"></randomforest>

                <pca plugin="plugins/manip/many_to_one_pca.py" tag="pca1-ozone">
                    <n_features type="int" value="11" />
                </pca>

                <candlestick plugin="plugins/manip/many_to_one_candlestick.py" tag="cand1-ozone">
                    <time_interval type="int" value="20" />
                </candlestick>
            </model>
        </dataset>
    </train>
</simu>
