<?xml version="1.0" encoding="UTF-8"?>
<simu>
    <train>
        <dataset file="test/datasets/google-stock-xgb/xgb1.csv">
            <model plugin="plugins/models/RNN/LSTM.py">
                <name>barebones_model</name>

                <algorithm>LSTM</algorithm>

                <parameters>
                    <squence_length>12</squence_length>
                    <batch_size>256</batch_size>
                    <epochs>50</epochs>
                    <validation_split>0.1</validation_split>
                    <verbose>1</verbose>
                    <learning_rate>0.001</learning_rate>
                </parameters>
            </model>
        </dataset>
    </train>
</simu>
